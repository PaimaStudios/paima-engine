{
  "name": "@paima/engine",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/node-sdk/paima-engine/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/node-sdk/paima-engine/dist",
        "main": "packages/node-sdk/paima-engine/src/index.ts",
        "tsConfig": "packages/node-sdk/paima-engine/tsconfig.lib.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "packages/engine/paima-runtime/public",
            "output": "public"
          }
        ],
        "format": ["cjs"]
      }
    }
  }
}
