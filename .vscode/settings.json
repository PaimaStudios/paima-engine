{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    ".env.*": "properties"
  },
  "[sql]": {
    "editor.formatOnSave": false
  },
  "[json][jsonc][json5]": {
    "editor.formatOnSave": true
  },
  "eslint.experimental.useFlatConfig": true,
}
